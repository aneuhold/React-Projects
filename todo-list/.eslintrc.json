{
    "extends": "airbnb",
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/destructuring-assignment": ["enabled", "never"],
        "react/forbid-prop-types": [false]
    },
    "env": {
        "browser": true,
        "node": true,
        "jest": true
    },
    "parser": "babel-eslint"
}